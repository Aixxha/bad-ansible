Host bastion
  Hostname bastion.GUID.example.opentlc.com
  User ankay-redhat.com
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent yes
  ControlMaster auto
  ControlPath /tmp/%h-%r
  ControlPersist 5m
  StrictHostKeyChecking no


Host *.internal
  User ec2-user
  IdentityFile ./GUIDkey.pem
  ProxyJump bastion
  StrictHostKeyChecking no

